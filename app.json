{
  "expo": {
    "name": "JPDict",
    "description": "Skylark JPDict",
    "slug": "JPDictRn",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/JPDict.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skylarkworkshop.jpdict",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.kevingaojx.jpdict",
      "permissions": [],
      "versionCode": 1
    },
    "packagerOpts": {
      "assetExts": [
        "db",
        "txt"
      ]
    }
  }
}
