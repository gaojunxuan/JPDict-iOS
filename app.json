{
  "expo": {
    "name": "JPDict",
    "description": "Skylark JPDict",
    "slug": "JPDictRn",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/JPDict-new.png",
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skylarkworkshop.jpdict",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.kevingaojx.jpdict",
      "permissions": [],
      "versionCode": 1
    },

    "extra": {
      "JPDICT_API_KEY": "[API KEY]"
    }
  }
}
